{
  "name": "CennznetIdentityProvider",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "NODE_OPTIONS=--max_old_space_size=8192 react-native start",
    "test": "rm -rf coverage && jest --coverage",
    "format": "prettier --write 'src/**/*.tsx' && prettier --write 'src/**/*.ts'",
    "format-check": "prettier --check 'src/**/*.tsx' && prettier --check  'src/**/*.ts'",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "android:release": "cd android && ./gradlew assembleRelease",
    "android:debug": "cd android && ./gradlew assembleDebug",
    "clean": "watchman watch-del-all && rm -rf node_modules $TMPDIR/metro-* $TMPDIR/haste-map-*",
    "start-cli": "node --max-old-space-size=8192 node_modules/react-native/local-cli/cli.js start",
    "integration:package": "cd integration && npm run package"
  },
  "dependencies": {
